
create procedure FindSupplierIngredients

    @supplier_name varchar(20)

as

begin



    ((SELECT *
    from Supplier JOIN Supplies 
    where SupplierID = Supplies.SupplierID and @supplier_name=name)
    as A
    (SELECT *
    from A JOIN Ingredients 
    where A.IngredientsID = Ingredients.ID)
    )


end